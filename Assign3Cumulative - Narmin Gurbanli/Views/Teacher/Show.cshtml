@model Assign3Cumulative___Narmin_Gurbanli.Models.Teacher
@{
    ViewBag.Title = "Show";
}

<a class="navlink" href="/Teacher/List">Back to Teachers</a>
<div id="containershow">
    <h2>@Model.TeacherFname @Model.TeacherLname</h2>
    <p>This teacher's employee number is @Model.EmployeeNumber</p>
    <p>This teacher was hired on @Model.FormattedHireDate</p>
    <p>This teacher's salary is @Model.FormattedSalary per hour</p>
    
</div>





